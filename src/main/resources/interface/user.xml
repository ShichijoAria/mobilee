<?xml version="1.0" encoding="UTF-8"?>
<root>
    <render title="网站用户" extra="Website User" icon="user" namespace="user"/>
    <properties>
        <property key="id" value="用户编号" type="number" head="true" show="true" edit="false" range="false" class="long"/>
        <property key="name" value="用户姓名" type="text" head="true" show="true" edit="true"  range="false"  class="String"/>
        <property key="gender" value="用户性别" type="select" head="true" show="true" edit="false"  range="false"  class="long">
            <enum key="1" value="男"/>
            <enum key="2" value="女"/>
        </property>
        <property key="created" value="入站时间" type="datetime" head="true" show="true" edit="false"  range="true"  class="Date"/>
        <property key="password" value="用户密码" type="text" head="false" show="true" edit="false"  range="false"  class="String"/>
        <property key="birthday" value="出生日期" type="date" head="false" show="true" edit="false"  range="false"  class="Date"/>
        <property key="phone" value="手机号码" type="text" head="false" show="true" edit="false"  range="false"  class="String"/>
        <property key="email" value="电子邮箱" type="text" head="false" show="true" edit="false"  range="false" class="String"/>
        <property key="headPortrait" value="用户头像" type="img" head="false" show="true" edit="true"  range="false"  class="String"/>
    </properties>
    <grant>
        <update>true</update>
        <insert>false</insert>
        <delete>false</delete>
    </grant>
</root>